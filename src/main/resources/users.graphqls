type Query {
    users: [User]
    getUser(id:Int!):User
}

type User {
    id: Int!
    name: String
    username:String
    email:String
    address:Address
    phone:String
    website:String
    company:Company
}

type Company {
    name:String
    catchPhrase:String
    bs:String
}
type Address {
    street:String
    suite:String
    city:String
    zipcode:String
    geo:Geography
}
type Geography {
    lat:String
    lng:String
}
